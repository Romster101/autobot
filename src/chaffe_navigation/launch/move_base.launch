<launch>
  <!-- move_base -->
    <!--rosparam file="$(find chaffe_navigation)/param/chaffe_params.yaml" command="load" /-->

 <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
    

    <rosparam file="$(find chaffe_navigation)/params/global_costmap_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find chaffe_navigation)/params/local_costmap_params.yaml" command="load" ns="local_costmap" />
	
    <rosparam file="$(find chaffe_navigation)/params/dwa_local_planner_params_burger.yaml" command="load" />
  </node>

</launch>
